<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <appender name="CONSOLE_PLAIN" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d, %-5p, %c:%L, %t] - %mdc %m%n</pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="CONSOLE_PLAIN"/>
    </root>

    <logger name="no.nav.pensjon.selvbetjening.skattetrekk" level="INFO"/>

</configuration>
