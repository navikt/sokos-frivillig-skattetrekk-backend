    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "stylelint-config-standard": "^38.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.2",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-plugin-stylelint": "^6.0.0",
    "vitest": "^3.1.2"
