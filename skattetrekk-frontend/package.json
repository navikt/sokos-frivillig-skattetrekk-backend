    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.0",
    "node-jose": "^2.2.0",
    "openid-client": "5.7.0",
    "react-router-dom": "^7.6.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@navikt/aksel-icons": "^7.14.2",
    "@navikt/ds-css": "^7.14.2",
    "@navikt/ds-react": "^7.14.2",
    "@navikt/nav-dekoratoren-moduler": "^3.2.2",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
