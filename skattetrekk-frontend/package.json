{
  "name": "skattetrekk-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "src/server/app.js",
  "scripts": {
    "dev-local": "concurrently --kill-others-on-fail  \"vite --port 8080 \" \"node mock/server.cjs\"",
    "dev": "vite --port 8080",
    "dev-json": "node run.cjs",
    "build": "tsc && vite build",
    "build:production": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "node-jose": "^2.2.0",
    "openid-client": "6.4.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@navikt/aksel-icons": "^5.9.2",
    "@navikt/ds-css": "^5.7.6",
    "@navikt/ds-react": "^5.7.6",
    "@navikt/nav-dekoratoren-moduler": "^2.1.3",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.4.0",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^6.0.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^6.0.0",
    "vitest": "^3.1.1"
  }
}
