{
  "name": "skattetrekk-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "src/server/app.js",
  "scripts": {
    "dev-local": "concurrently --kill-others-on-fail  \"vite --port 8080 \" \"node mock/server.cjs\"",
    "dev": "vite --port 8080",
    "dev-json": "node run.cjs",
    "dev-local-backend-mot-q2": "vite --port 3000",
    "build": "tsc && vite build --mode development",
    "build:production": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.0",
    "node-jose": "^2.2.0",
    "openid-client": "5.7.0",
    "react-router-dom": "^7.7.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@navikt/aksel-icons": "^7.14.2",
    "@navikt/ds-css": "^7.14.2",
    "@navikt/ds-react": "^7.14.2",
    "@navikt/nav-dekoratoren-moduler": "^3.2.2",
    "@types/express": "^5.0.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "stylelint-config-standard": "^38.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-eslint2": "^5.0.4",
    "vite-plugin-stylelint": "^6.0.2",
    "vitest": "^3.2.4"
  }
